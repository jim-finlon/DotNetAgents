{
  "dashboard": {
    "title": "DotNetAgents - Agents",
    "tags": ["dotnetagents", "agents"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Agent Status",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "dotnetagents_agents_status{status=\"available\"}",
            "format": "table",
            "legendFormat": "Available"
          },
          {
            "expr": "dotnetagents_agents_status{status=\"busy\"}",
            "format": "table",
            "legendFormat": "Busy"
          },
          {
            "expr": "dotnetagents_agents_status{status=\"idle\"}",
            "format": "table",
            "legendFormat": "Idle"
          }
        ]
      },
      {
        "id": 2,
        "title": "Tasks by Agent",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "dotnetagents_agent_tasks_total",
            "legendFormat": "{{agent_id}}"
          }
        ]
      },
      {
        "id": 3,
        "title": "Agent Task Success Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(dotnetagents_agent_tasks_success_total[5m]) / rate(dotnetagents_agent_tasks_total[5m]) * 100",
            "legendFormat": "{{agent_id}}"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Success Rate %"}
        ]
      },
      {
        "id": 4,
        "title": "Agent Response Time",
        "type": "graph",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(dotnetagents_agent_task_duration_seconds_bucket[5m]))",
            "legendFormat": "{{agent_id}} p95"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"}
        ]
      }
    ]
  }
}
