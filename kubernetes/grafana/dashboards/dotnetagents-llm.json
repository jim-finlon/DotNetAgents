{
  "dashboard": {
    "title": "DotNetAgents - LLM Performance",
    "tags": ["dotnetagents", "llm"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "LLM Call Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(dotnetagents_llm_calls_total[5m])",
            "legendFormat": "{{model}}"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Calls/sec"}
        ]
      },
      {
        "id": 2,
        "title": "LLM Call Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(dotnetagents_llm_call_duration_seconds_bucket[5m]))",
            "legendFormat": "{{model}} p95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(dotnetagents_llm_call_duration_seconds_bucket[5m]))",
            "legendFormat": "{{model}} p50"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"}
        ]
      },
      {
        "id": 3,
        "title": "LLM Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "rate(dotnetagents_llm_calls_failed_total[5m]) / rate(dotnetagents_llm_calls_total[5m]) * 100",
            "legendFormat": "{{model}}"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Error Rate %"}
        ]
      },
      {
        "id": 4,
        "title": "LLM Token Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(dotnetagents_llm_tokens_total[5m])",
            "legendFormat": "{{model}}"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Tokens/sec"}
        ]
      },
      {
        "id": 5,
        "title": "LLM Cost per Hour",
        "type": "stat",
        "gridPos": {"h": 4, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "rate(dotnetagents_cost_total_usd[1h]) * 3600",
            "legendFormat": "Cost"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        }
      }
    ]
  }
}
